{
	"name": "freecodecamp dark",
	"type": "dark",
	"colors": {
		"editor.background": "#2a2a40",
		"editor.foreground": "#d4d4d4",
		"activityBar.background": "#0a0a23",
		"activityBar.inactiveForeground": "#ffffffa0",
		"button.background": "#3b3b4f",
		"button.border": "#f5f6f7",
		"editor.lineHighlightBorder": "#0e4470",
		"editorBracketMatch.background": "#263039",
		"editorGroupHeader.tabsBackground": "#1b1b32",
		"editorGroupHeader.tabsBorder": "#dddddd",
		"tab.activeBackground": "#3b3b4f",
		"tab.inactiveBackground": "#1b1b32",
		"tab.hoverBackground": "#eeeeee",
		"tab.hoverForeground": "#1b1b32",
		"tab.activeBorderTop": "#dddddd",
		"tab.activeBorder": "#3b3b4f",
		"editorIndentGuide.background1": "#ffffff00",
		"editorIndentGuide.activeBackground1": "#ffffff00",
		"editorLineNumber.foreground": "#ffffff",
		"editorLineNumber.activeForeground": "#c6c6c6",
		"editorWidget.background": "#0a0a23",
		"input.background": "#3b3b4f",
		"panel.background": "#1b1b32",
		"sideBar.background": "#1b1b32",
		"sideBar.border": "#3b3b4f"
	},
	"tokenColors": [
		{
			"name": "Comments, JSDoc",
			"scope": [
				"comment",
				"storage.type.class.jsdoc",
				"variable.other.jsdoc"
			],
			"settings": {
				"foreground": "#608b4e"
			}
		},
		{
			"name": "Keywords, Data Types, Booleans",
			"scope": [
				"keyword",
				"keyword.operator.new",
				"storage",
				"constant.language"
			],
			"settings": {
				"foreground": "#569cd6"
			}
		},
		{
			"name": "Operators",
			"scope": [
				"keyword.operator",
				"storage.type.function.arrow"
			],
			"settings": {
				"foreground": "#9cdcfe"
			}
		},
		{
			"name": "Numbers",
			"scope": [
				"constant.numeric"
			],
			"settings": {
				"foreground": "#b5cea8"
			}
		},
		{
			"name": "Strings",
			"scope": [
				"string"
			],
			"settings": {
				"foreground": "#ce9178"
			}
		},
		{
			"name": "Tags Punctuation",
			"scope": [
				"punctuation.definition.tag"
			],
			"settings": {
				"foreground": "#808080"
			}
		},
		{
			"name": "Tags, Selector",
			"scope": [
				"entity.name.tag",
				"meta.selector"
			],
			"settings": {
				"foreground": "#569cd6"
			}
		},
		{
			"name": "Properties",
			"scope": [
				"meta.property-name"
			],
			"settings": {
				"foreground": "#9cdcfe"
			}
		},
		{
			"name": "Values, Attribute Selector",
			"scope": [
				"meta.property-value",
				"meta.attribute-selector"
			],
			"settings": {
				"foreground": "#ce9178"
			}
		},
		{
			"name": "Defaults - Variable",
			"scope": [
				"variable"
			],
			"settings": {
				"foreground": "#d4d4d4"
			}
		},
		{
			"name": "JS: Colored Punctuation",
			"scope": [
				"punctuation.terminator.statement.js",
				"punctuation.accessor.js",
				"punctuation.separator.key-value.js",
				"punctuation.separator.comma.js"
			],
			"settings": {
				"foreground": "#9cdcfe"
			}
		},
		{
			"name": "JS: Colored Brackets",
			"scope": [
				"punctuation.definition.parameters.begin.js",
				"punctuation.definition.parameters.end.js",
				"punctuation.definition.block.js",
				"meta.brace.round.js",
				"meta.template.expression.js"
			],
			"settings": {
				"foreground": "#ffff00"
			}
		},
		{
			"name": "CSS: :",
			"scope": [
				"punctuation.separator.key-value.css"
			],
			"settings": {
				"foreground": "#9cdcfe"
			}
		},
		{
			"name": "CSS: Numeric Units",
			"scope": [
				"keyword.other.unit"
			],
			"settings": {
				"foreground": "#b5cea8"
			}
		},
		{
			"name": "CSS: media, logical",
			"scope": [
				"meta.at-rule.media.header.css",
				"keyword.operator.logical.only.media.css",
				"keyword.operator.logical.and.media.css"
			],
			"settings": {
				"foreground": "#569cd6"
			}
		},
		{
			"name": "CSS: Punctuation",
			"scope": [
				"keyword.operator.arithmetic.css",
				"punctuation.separator.list.comma.css",
				"punctuation.definition.entity.begin.bracket.square.css",
				"punctuation.definition.entity.end.bracket.square.css"
			],
			"settings": {
				"foreground": "#dcdcdc"
			}
		},
		{
			"name": "CSS: Defaults",
			"scope": [
				"punctuation.definition.keyword",
				"keyword.operator.pattern.css",
				"constant.other.color.rgb-value.hex.css",
				"punctuation.definition.parameters.begin.bracket.round.css",
				"punctuation.definition.parameters.end.bracket.round.css"
			],
			"settings": {
				"foreground": "#d4d4d4"
			}
		},
		{
			"name": "HTML: Attributes",
			"scope": [
				"entity.other.attribute-name.html"
			],
			"settings": {
				"foreground": "#9cdcfe"
			}
		},
		{
			"name": "HTML: Attributes",
			"scope": [
				"punctuation.separator.key-value.html"
			],
			"settings": {
				"foreground": "#808080"
			}
		}
	]
}
