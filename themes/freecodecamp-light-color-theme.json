{
	"name": "freecodecamp light",
	"type": "light",
	"colors": {
		"editor.background": "#ffffff",
		"editor.foreground": "#000000",
		"activityBar.background": "#e6e6e6",
		"activityBar.foreground": "#000000",
		"activityBar.border": "#d0d0d5",
		"button.background": "#d0d0d5",
		"button.foreground": "#1b1b32",
		"button.border": "#1b1b32",
		"editor.lineHighlightBorder": "#cee8fc",
		"editorBracketMatch.background": "#e5efe5",
		"editorBracketHighlight.foreground1": "#000000",
		"editorBracketHighlight.foreground2": "#000000",
		"editorBracketHighlight.foreground3": "#000000",
		"editorBracketHighlight.foreground4": "#000000",
		"editorBracketHighlight.foreground5": "#000000",
		"editorBracketHighlight.foreground6": "#000000",
		"editorGroupHeader.tabsBackground": "#f5f6f7",
		"editorGroupHeader.tabsBorder": "#dddddd",
		"tab.activeBackground": "#d0d0d5",
		"tab.inactiveBackground": "#f5f6f7",
		"tab.activeBorderTop": "#dddddd",
		"tab.activeBorder": "#d0d0d5",
		"editorIndentGuide.background": "#ffffff00",
		"editorIndentGuide.activeBackground": "#ffffff00",
		"editorLineNumber.foreground": "#0a0a23",
		"editorLineNumber.activeForeground": "#0b216f",
		"editorWidget.background": "#f5f6f7",
		"panel.background": "#f5f6f7",
		"sideBar.background": "#f5f6f7",
		"sideBar.border": "#d0d0d5"
	},
	"tokenColors": [
		{
			"name": "Generic: Comments",
			"scope": [
				"comment"
			],
			"settings": {
				"foreground": "#008000"
			}
		},
		{
			"name": "Generic: Keywords, Data Types, Booleans",
			"scope": [
				"keyword",
				"keyword.operator.new",
				"storage",
				"constant.language"
			],
			"settings": {
				"foreground": "#0000ff"
			}
		},
		{
			"name": "Generic: Numbers",
			"scope": [
				"constant.numeric"
			],
			"settings": {
				"foreground": "#098658"
			}
		},
		{
			"name": "Generic: Strings",
			"scope": [
				"string"
			],
			"settings": {
				"foreground": "#a31515"
			}
		},
		{
			"name": "Generic: Tags Punctuation",
			"scope": [
				"punctuation.definition.tag"
			],
			"settings": {
				"foreground": "#383838"
			}
		},
		{
			"name": "Generic: Tags, Selector",
			"scope": [
				"entity.name.tag",
				"meta.selector"
			],
			"settings": {
				"foreground": "#800000"
			}
		},
		{
			"name": "Generic: Properties",
			"scope": [
				"meta.property-name"
			],
			"settings": {
				"foreground": "#ff0000"
			}
		},
		{
			"name": "Generic: Values, Attribute Selector",
			"scope": [
				"meta.property-value",
				"meta.attribute-selector"
			],
			"settings": {
				"foreground": "#0451a5"
			}
		},
		{
			"name": "JS: Foreground",
			"scope": [
				"source.js",
				"variable"
			],
			"settings": {
				"foreground": "#00107e"
			}
		},
		{
			"name": "JS: Defaults",
			"scope": [
				"keyword.operator",
				"punctuation.terminator",
				"punctuation.accessor"
			],
			"settings": {
				"foreground": "#000000"
			}
		},
		{
			"name": "CSS: Numeric Units",
			"scope": [
				"keyword.other.unit"
			],
			"settings": {
				"foreground": "#098658"
			}
		},
		{
			"name": "CSS: media, logical",
			"scope": [
				"keyword.control.at-rule.media.css",
				"meta.at-rule.media.header.css",
				"keyword.operator.logical.and.media.css",
				"keyword.operator.logical.only.media.css"
			],
			"settings": {
				"foreground": "#800000"
			}
		},
		{
			"name": "CSS: Defaults",
			"scope": [
				"punctuation.definition.keyword",
				"entity.name.tag.wildcard"
			],
			"settings": {
				"foreground": "#000000"
			}
		},
		{
			"name": "HTML: Strings",
			"scope": [
				"string.quoted.double.html"
			],
			"settings": {
				"foreground": "#0000ff"
			}
		},
		{
			"name": "HTML: Attributes",
			"scope": [
				"entity.other.attribute-name.html"
			],
			"settings": {
				"foreground": "#ff0000"
			}
		}
	]
}
