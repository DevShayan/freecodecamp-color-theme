{
	"name": "freecodecamp light",
	"type": "light",
	"colors": {
		"editor.background": "#ffffff",
		"editor.foreground": "#000000",
		"activityBar.background": "#e6e6e6",
		"activityBar.foreground": "#000000",
		"activityBar.border": "#d0d0d5",
		"button.background": "#d0d0d5",
		"button.foreground": "#1b1b32",
		"button.border": "#1b1b32",
		"editor.lineHighlightBorder": "#cee8fc",
		"editorBracketMatch.background": "#e5efe5",
		"editorGroupHeader.tabsBackground": "#f5f6f7",
		"editorGroupHeader.tabsBorder": "#dddddd",
		"tab.activeBackground": "#d0d0d5",
		"tab.inactiveBackground": "#f5f6f7",
		"tab.activeBorderTop": "#dddddd",
		"tab.activeBorder": "#d0d0d5",
		"editorIndentGuide.background1": "#ffffff00",
		"editorIndentGuide.activeBackground1": "#ffffff00",
		"editorLineNumber.foreground": "#0a0a23",
		"editorLineNumber.activeForeground": "#0b216f",
		"editorWidget.background": "#f5f6f7",
		"panel.background": "#f5f6f7",
		"sideBar.background": "#f5f6f7",
		"sideBar.border": "#d0d0d5"
	},
	"tokenColors": [
		{
			"name": "Comments, JSDoc",
			"scope": [
				"comment",
				"storage.type.class.jsdoc",
				"variable.other.jsdoc"
			],
			"settings": {
				"foreground": "#008000"
			}
		},
		{
			"name": "Keywords, Data Types, Booleans",
			"scope": [
				"keyword",
				"keyword.operator.new",
				"storage",
				"constant.language"
			],
			"settings": {
				"foreground": "#0000ff"
			}
		},
		{
			"name": "Numbers",
			"scope": [
				"constant.numeric"
			],
			"settings": {
				"foreground": "#098658"
			}
		},
		{
			"name": "Strings",
			"scope": [
				"string"
			],
			"settings": {
				"foreground": "#a31515"
			}
		},
		{
			"name": "Tags Punctuation",
			"scope": [
				"punctuation.definition.tag"
			],
			"settings": {
				"foreground": "#383838"
			}
		},
		{
			"name": "Tags, Selector",
			"scope": [
				"entity.name.tag",
				"meta.selector"
			],
			"settings": {
				"foreground": "#800000"
			}
		},
		{
			"name": "Properties",
			"scope": [
				"meta.property-name"
			],
			"settings": {
				"foreground": "#ff0000"
			}
		},
		{
			"name": "Values, Attribute Selector",
			"scope": [
				"meta.property-value",
				"meta.attribute-selector"
			],
			"settings": {
				"foreground": "#0451a5"
			}
		},
		{
			"name": "JS: Variable, Function",
			"scope": [
				"variable",
				"support.variable.property.js",
				"entity.name.function.js"
			],
			"settings": {
				"foreground": "#00107e"
			}
		},
		{
			"name": "JS: Defaults",
			"scope": [
				"keyword.operator",
				"storage.type.function.arrow.js",
				"meta.template.expression.js"
			],
			"settings": {
				"foreground": "#000000"
			}
		},
		{
			"name": "CSS: :",
			"scope": [
				"punctuation.separator.key-value.css"
			],
			"settings": {
				"foreground": "#ff0000"
			}
		},
		{
			"name": "CSS: Numeric Units",
			"scope": [
				"keyword.other.unit"
			],
			"settings": {
				"foreground": "#098658"
			}
		},
		{
			"name": "CSS: media, logical",
			"scope": [
				"keyword.control.at-rule.media.css",
				"meta.at-rule.media.header.css",
				"keyword.operator.logical.and.media.css",
				"keyword.operator.logical.only.media.css"
			],
			"settings": {
				"foreground": "#800000"
			}
		},
		{
			"name": "CSS: Defaults",
			"scope": [
				"punctuation.definition.keyword",
				"punctuation.definition.parameters.begin.bracket.round.css",
				"punctuation.definition.parameters.end.bracket.round.css",
				"punctuation.separator.list.comma.css",
				"punctuation.definition.entity.begin.bracket.square.css",
				"punctuation.definition.entity.end.bracket.square.css"
			],
			"settings": {
				"foreground": "#000000"
			}
		},
		{
			"name": "HTML: Strings",
			"scope": [
				"string.quoted.double.html"
			],
			"settings": {
				"foreground": "#0000ff"
			}
		},
		{
			"name": "HTML: Attributes",
			"scope": [
				"entity.other.attribute-name.html"
			],
			"settings": {
				"foreground": "#ff0000"
			}
		}
	]
}
