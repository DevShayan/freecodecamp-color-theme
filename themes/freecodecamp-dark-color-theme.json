{
	"name": "freecodecamp dark",
	"type": "dark",
	"colors": {
		"editor.background": "#2a2a40",
		"editor.foreground": "#d4d4d4",
		"activityBar.background": "#0a0a23",
		"activityBar.inactiveForeground": "#ffffffa0",
		"button.background": "#3b3b4f",
		"button.border": "#f5f6f7",
		"editor.lineHighlightBorder": "#0e4470",
		"editorBracketMatch.background": "#263039",
		"editorBracketHighlight.foreground1": "#ffff00",
		"editorBracketHighlight.foreground2": "#ffff00",
		"editorBracketHighlight.foreground3": "#ffff00",
		"editorBracketHighlight.foreground4": "#ffff00",
		"editorBracketHighlight.foreground5": "#ffff00",
		"editorBracketHighlight.foreground6": "#ffff00",
		"editorGroupHeader.tabsBackground": "#1b1b32",
		"editorGroupHeader.tabsBorder": "#dddddd",
		"tab.activeBackground": "#3b3b4f",
		"tab.inactiveBackground": "#1b1b32",
		"tab.hoverBackground": "#eeeeee",
		"tab.hoverForeground": "#1b1b32",
		"tab.activeBorderTop": "#dddddd",
		"tab.activeBorder": "#3b3b4f",
		"editorIndentGuide.background": "#ffffff00",
		"editorIndentGuide.activeBackground": "#ffffff00",
		"editorLineNumber.foreground": "#ffffff",
		"editorLineNumber.activeForeground": "#c6c6c6",
		"editorWidget.background": "#0a0a23",
		"input.background": "#3b3b4f",
		"panel.background": "#1b1b32",
		"sideBar.background": "#1b1b32",
		"sideBar.border": "#3b3b4f"
	},
	"tokenColors": [
		{
			"name": "Generic: Comments",
			"scope": [
				"comment"
			],
			"settings": {
				"foreground": "#608b4e"
			}
		},
		{
			"name": "Generic: Keywords, Data Types, Booleans",
			"scope": [
				"keyword",
				"keyword.operator.new",
				"storage",
				"constant.language"
			],
			"settings": {
				"foreground": "#569cd6"
			}
		},
		{
			"name": "Generic: Operators",
			"scope": [
				"keyword.operator",
			],
			"settings": {
				"foreground": "#9cdcfe"
			}
		},
		{
			"name": "Generic: Numbers",
			"scope": [
				"constant.numeric"
			],
			"settings": {
				"foreground": "#b5cea8"
			}
		},
		{
			"name": "Generic: Strings",
			"scope": [
				"string"
			],
			"settings": {
				"foreground": "#ce9178"
			}
		},
		{
			"name": "Generic: Tags Punctuation",
			"scope": [
				"punctuation.definition.tag"
			],
			"settings": {
				"foreground": "#808080"
			}
		},
		{
			"name": "Generic: Tags, Selector",
			"scope": [
				"entity.name.tag",
				"meta.selector"
			],
			"settings": {
				"foreground": "#569cd6"
			}
		},
		{
			"name": "Generic: Properties",
			"scope": [
				"meta.property-name"
			],
			"settings": {
				"foreground": "#9cdcfe"
			}
		},
		{
			"name": "Generic: Values, Attribute Selector",
			"scope": [
				"meta.property-value",
				"meta.attribute-selector"
			],
			"settings": {
				"foreground": "#ce9178"
			}
		},
		{
			"name": "Generic: Defaults",
			"scope": [
				"variable"
			],
			"settings": {
				"foreground": "#d4d4d4"
			}
		},
		{
			"name": "JS: Colored Punctuation",
			"scope": [
				"punctuation.terminator.statement.js",
				"punctuation.accessor.js"
			],
			"settings": {
				"foreground": "#9cdcfe"
			}
		},
		{
			"name": "CSS: Numeric Units",
			"scope": [
				"keyword.other.unit"
			],
			"settings": {
				"foreground": "#b5cea8"
			}
		},
		{
			"name": "CSS: media, logical",
			"scope": [
				"meta.at-rule.media.header.css",
				"keyword.operator.logical.only",
				"keyword.operator.logical.and"
			],
			"settings": {
				"foreground": "#569cd6"
			}
		},
		{
			"name": "CSS: Defaults",
			"scope": [
				"punctuation.definition.keyword",
				"entity.name.tag.wildcard",
				"keyword.operator.pattern.css",
				"punctuation.separator.list.comma.css",
				"constant.other.color.rgb-value.hex.css"
			],
			"settings": {
				"foreground": "#d4d4d4"
			}
		},
		{
			"name": "HTML: Attributes",
			"scope": [
				"entity.other.attribute-name.html"
			],
			"settings": {
				"foreground": "#9cdcfe"
			}
		}
	]
}
